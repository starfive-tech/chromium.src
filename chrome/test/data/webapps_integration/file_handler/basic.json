{
  "name": "File Handler",
  "icons": [
    {
      "src": "basic-48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "basic-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "/webapps_integration/file_handler/basic.html",
  "scope": "/webapps_integration/file_handler/",
  "display": "minimal-ui",
  "file_handlers": [
    {
      "action": "/webapps_integration/file_handler/text_handler.html",
      "name": "Plain Text",
      "accept": {
        "text/plain": [".txt", ".md", ".csv", ".text"],
        "application/octet-stream": [".qux", ".quux"]
      }
    },
    {
      "action": "/webapps_integration/file_handler/image_handler.html",
      "accept": {
        "image/png": [".png"]
      },
      "launch_type": "multiple-clients"
    }
  ]
}
